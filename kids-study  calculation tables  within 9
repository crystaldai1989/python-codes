#九九加法表
for x in range(1,10):
    for y in range(1,x+1):
        print('{}+{}={}'.format(x,y,x+y),end='   ')
    print(' ')

#九九减法表
for x in range(1,10):
    for y in range(1,x+1):
        if x-y>=0:
            print('{}-{}={}'.format(x,y,x-y),end='   ')
    print(' ')

#九九乘法表
for x in range(1,10):
    for y in range(1,x+1):
        print('{}X{}={}'.format(x,y,x*y),end=' ')
    print(' ')


